    "lint": "eslint src/**/*.js",
    "lint:ci": "eslint src/**/*.js --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.js",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
