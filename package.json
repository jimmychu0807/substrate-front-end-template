    "@polkadot/keyring": "^8.2.2",
    "@polkadot/networks": "^8.2.2",
    "@polkadot/types": "^6.12.1",
    "@polkadot/ui-keyring": "^0.87.6",
    "@polkadot/ui-settings": "^0.87.6",
    "@polkadot/util": "^8.2.2",
    "@polkadot/util-crypto": "^8.2.2",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
